        "def prime(n):\n",
        "  a = []\n",
        "  i = int(2)\n",
        "  while (len(a)<n):\n",
        "    count = 0\n",
        "    for j in range (1,int(i**(1/2) + 1)):\n",
        "      if (i%j == 0):\n",
        "        count += 1\n",
        "\n",
        "    if (count==1):\n",
        "      a.append(i)\n",
        "    i = i+1\n",
        "  return a[-1]\n",
        "\n",
        "\n",
        "print(prime(100))"
      ],
